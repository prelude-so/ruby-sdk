module PreludeSDK
  module Type
    module Union
      include PreludeSDK::Type::Converter

      private def self.known_variants: -> ::Array[[Symbol?, (^-> PreludeSDK::Type::Converter::input)]]

      def self.derefed_variants: -> ::Array[[Symbol?, top]]

      def self.variants: -> ::Array[top]

      private def self.discriminator: (Symbol property) -> void

      private def self.variant: (
        Symbol
        | ::Hash[Symbol, top]
        | ^-> PreludeSDK::Type::Converter::input
        | PreludeSDK::Type::Converter::input key,
        ?::Hash[Symbol, top]
        | ^-> PreludeSDK::Type::Converter::input
        | PreludeSDK::Type::Converter::input spec
      ) -> void

      private def self.resolve_variant: (
        top value
      ) -> PreludeSDK::Type::Converter::input?

      def ===: (top other) -> bool

      def ==: (top other) -> bool

      def coerce: (top value, state: PreludeSDK::Type::Converter::state) -> top

      def dump: (top value) -> top
    end
  end
end
